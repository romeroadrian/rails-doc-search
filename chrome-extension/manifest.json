{
  "manifest_version": 2,
  "name": "Ruby on Rails API Search",
  "description": "Search the Ruby on Rails API Documentation",
  "version": "0.1.3",
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions": [
    "tabs",
    "http://api.rubyonrails.org/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "128": "icons/128.png"
  },
  "omnibox": {
    "keyword": "rd"
  },
  "background": {
    "scripts": ["jquery.min.js","searcher.js", "search_index.js", "background.js", "ga.js"]
  }
}
